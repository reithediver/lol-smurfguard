{
  "name": "league-smurf-detector",
  "version": "0.1.0",
  "description": "A web application that detects League of Legends smurf accounts using AI-driven analysis",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "npm run build",
    "test-summoners": "ts-node src/test-summoners.ts",
    "verify-key": "ts-node src/verify-api-key.ts",
    "test-api": "ts-node src/test-api-access.ts",
    "health-check": "ts-node src/utils/health-check.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "league-of-legends-api-wrapper": "^0.0.4",
    "mongoose": "^8.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.0",
    "@types/winston": "^2.4.4",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "",
  "license": "MIT"
}
